int doubleVal(int val) {
  val = val*2;
  return val;
}

float calcCircumf(float r) {
  float circumf = (r*2)*3.14;
  return circumf;
}

boolean isNegative(float val) {
  boolean neg;
  if(val < 0) {
    neg = true;
  } else {
    neg = false;
  }
  return neg;
}

float halfVal(float val) {
  val = val/2;
  return val;
}